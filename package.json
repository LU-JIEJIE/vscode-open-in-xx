{
  "publisher": "LuJiejie",
  "name": "open-in-xx",
  "displayName": "Open in XX",
  "version": "0.0.4",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "description": "Help you open the links that associated with current repository conveniently.",
  "author": "LU-JIEJIE",
  "license": "MIT",
  "homepage": "https://github.com/LU-JIEJIE/vscode-open-in-xx#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/LU-JIEJIE/vscode-open-in-xx"
  },
  "bugs": {
    "url": "https://github.com/LU-JIEJIE/vscode-open-in-xx/issues"
  },
  "categories": [
    "Other"
  ],
  "l10n": "./l10n",
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.88.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "menus": {
      "commandPalette": [
        {
          "command": "open-in-xx.showLinks"
        }
      ]
    },
    "commands": [
      {
        "category": "Open in XX",
        "shortTitle": "Show links",
        "command": "open-in-xx.showLinks",
        "title": "Show links associated with the repository"
      }
    ],
    "configuration": {
      "title": "Open in XX",
      "properties": {
        "open-in-xx.auto": {
          "type": "boolean",
          "default": true,
          "description": "Whether to automatically generate the links that may be associated with the repository"
        },
        "open-in-xx.links": {
          "type": "array",
          "description": "The links associated with the repository",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "The name of the link item"
              },
              "url": {
                "type": "string",
                "description": "The url of the link item"
              },
              "icon": {
                "type": "string",
                "description": "The icon of the link item",
                "enum": [
                  "tampermonkey",
                  "bilibili",
                  "npm",
                  "gitlab",
                  "gitee",
                  "pypi",
                  "discord",
                  "greasyfork",
                  "twitter",
                  "x",
                  "facebook",
                  "youtube",
                  "yuque",
                  "feishu",
                  "maven",
                  "docker",
                  "tieba",
                  "netlify",
                  "marketplace"
                ]
              }
            }
          }
        }
      }
    },
    "icons": {
      "icon-tampermonkey": {
        "description": "tampermonkey icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E800"
        }
      },
      "icon-bilibili": {
        "description": "bilibili icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E801"
        }
      },
      "icon-npm": {
        "description": "npm icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E802"
        }
      },
      "icon-gitlab": {
        "description": "gitlab icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E803"
        }
      },
      "icon-gitee": {
        "description": "gitee icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E804"
        }
      },
      "icon-pypi": {
        "description": "pypi icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E805"
        }
      },
      "icon-discord": {
        "description": "discord icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E806"
        }
      },
      "icon-greasyfork": {
        "description": "greasyfork icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E807"
        }
      },
      "icon-twitter": {
        "description": "twitter icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E808"
        }
      },
      "icon-x": {
        "description": "x icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E809"
        }
      },
      "icon-facebook": {
        "description": "facebook icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E80A"
        }
      },
      "icon-youtube": {
        "description": "youtube icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E80B"
        }
      },
      "icon-yuque": {
        "description": "yuque icon",
        "default": {
          "fontPath": "./asset/myiconfont.woff",
          "fontCharacter": "\\E80C"
        }
      },
      "icon-feishu": {
